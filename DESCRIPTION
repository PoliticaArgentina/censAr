Package: censAr
Title: Base de Datos de Facil Acceso de Censos de Arentina
    (ArgentinaÂ´s Census Easy Access Database)
Version: 0.0.0.9
Authors@R: c(
        person(given = "Juan Pablo",
               family = "Ruiz Nicolini",
               role = c("aut", "cre", "cph"),
               email = "juanpabloruiznicolini@gmail.com")
